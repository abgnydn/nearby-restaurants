const r=({propertyName:r,link:e,target:t=document.body,defer:o=!0,async:a=!0},c)=>{if(n(r))return c();if(Array.from(document.scripts).some((r=>{if(r.src===e)return r.addEventListener("load",c),!0})))return;const i=document.createElement("script");i.src=e,i.type="text/javascript",i.addEventListener("load",c),i.defer=o,i.async=a,i.onerror=()=>console.warn("error loading resource: "+e),t.appendChild(i)},e=r=>r.toString().concat("px"),n=r=>r&&r.includes(".")?!!r.split(".").reduce(((r,e)=>r.hasOwnProperty(e)?r[e]:null),window):window.hasOwnProperty(r);function t(r){return{href:r}}export{t as h,r as i,e as p}